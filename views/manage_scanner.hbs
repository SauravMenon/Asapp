    <div class="container pt-4">
    <div style="border-top:solid 1px white" class="row">
    <div style="height:100px;border-right: solid 1px white" class="col-3 mt-3">
    <ul class="list-group list-group-flush">
    <a href="/manage/requests" class="list-group-item list-group-item-action item">Requests</a>
    <a href="" class="list-group-item list-group-item-action item">Scanners</a>
    <a href="/manage/places" class="list-group-item list-group-item-action item">Places</a>
    </ul>
    </div>


    <div class="col-9 mt-4">
        <h3 class="text-light">Scanners</h3>
    <div class="container">
    <button style="border-radius:25px" class="btn btn-outline-light" data-toggle="modal" data-target="#addplace">Add Scanner</button>
    <div class="modal fade" id="addplace">
    <div class="modal-dialog">
    <div class="modal-content modal-style">
    <div class="modal-header">
    <h5 class="">Enter staff details.</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
    </button>
    </div>
    <form action="" method="post">
    <div class="modal-body">
    <div class="form-group">
    <label for="name">ACCESS code</label>
    <input type="text" class="form-control" name="accessCode">
    </div>
    <div class="form-group">
    <label for="scanning">select place</label>
    <select class="form-control" name="scanning" id="">
        {{#each places}}
        <option value="{{this._id}}">{{this.name}}</option>
        {{/each}}
    </select>
    </div>


    </div>
    <div class="modal-footer">
    <button type="button" class="btn btn-secondary w-50" data-dismiss="modal">Cancel</button>
    <input type="submit" value="CREATE" class="form-control btn btn-primary w-50">
    </div>
    </form>
    </div>

    </div>
    </div>
    </div>

    <table class="table mt-2 text-light">
    <thead>
    <tr>
    <th scope="col">Scanning</th>
    <th scope="col">Access code</th>
    </tr>
    </thead>
    <tbody id="scanner-list">
    <!--
    <tr scope="row" class="trow">
    <td>mark</td>
    <td>mark</td>
    </tr>
    -->
     {{#each scanners}}
    <tr scope="row" class="trow" id="{{this._id}}">
       <td>{{this.name}}</td>
       <td>{{this.accessCode}}<button id="del-scanner" style="border-radius:25px" class="del btn btn-sm btn-outline-light">DELETE</button></td>
    </tr>
    {{/each}}
    </tbody>
    </table>
    </div>
    </div>
    </div>
    </body>
    <script src="/js/org-client.js"></script>
    </html>
